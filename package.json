{
  "name": "aptitude-hub",
  "version": "1.0.0",
  "description": "Complete Aptitude Testing Platform with Admin Panel",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "scss": "sass --watch scss:public/css",
    "seed": "node db/seed.js",
    "seed:improved": "node db/improvedQuestionGenerator.js",
    "migrate": "node db/schema.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "validator": "^13.11.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "sass": "^1.69.5"
  }
}
